{
  "collectCoverage": true,
  "coverageDirectory": "coverage",
  "collectCoverageFrom": [
    "**/lib/**/*.js",
    "**/lib/**/*.cjs"
  ],
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.c?js$",
  "testPathIgnorePatterns": [
    "/node_modules/",
    "__tests__/__mocks__",
    "__tests__/test-locally-only.cjs"
  ]
}